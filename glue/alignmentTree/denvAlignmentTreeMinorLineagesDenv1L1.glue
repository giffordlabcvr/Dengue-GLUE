  ### DENV1 constrained alignment tree: minor lineage-level alignments

  alignment AL_DENV_1I_E

    set field displayName "1I_E"
    set field clade_category "major_lineage"

	extract child AL_DENV_1I_E1 -r REF_DENV_1I_E.1_OQ426604
	demote member AL_DENV_1I_E1 --whereClause "sequence.minor_lineage = '1I_E.1'"

	extract child AL_DENV_1I_E2 -r REF_DENV_1I_E.2_MW315188
	demote member AL_DENV_1I_E2 --whereClause "sequence.minor_lineage = '1I_E.2'"
 
	exit
  
