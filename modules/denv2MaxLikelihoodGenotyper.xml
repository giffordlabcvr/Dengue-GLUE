<maxLikelihoodGenotyper>
	<maxLikelihoodPlacerModuleName>denv2MaxLikelihoodPlacer</maxLikelihoodPlacerModuleName>

	<cladeCategory>
		<name>serotype</name>	
		<displayName>Serotype</displayName>	
		<whereClause>displayName = '2'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
		<distanceCutoff>0.04</distanceCutoff>
		<finalCladeCutoff>70.0</finalCladeCutoff>
	</cladeCategory>
	
	<cladeCategory>
		<name>genotype</name>	
		<displayName>Genotype</displayName>	
		<whereClause>displayName like '2%' and clade_category = 'genotype'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
		<distanceCutoff>0.026</distanceCutoff>
		<finalCladeCutoff>60.0</finalCladeCutoff>
	</cladeCategory>


</maxLikelihoodGenotyper>



