<maxLikelihoodGenotyper>
	<maxLikelihoodPlacerModuleName>denv3MaxLikelihoodPlacer</maxLikelihoodPlacerModuleName>
	<useSingleReference>true</useSingleReference>

	<cladeCategory>
		<name>serotype</name>	
		<displayName>Serotype</displayName>	
		<whereClause>displayName = '3'</whereClause>
		<distanceScalingExponent>-4.5</distanceScalingExponent>
		<distanceCutoff>0.04</distanceCutoff>
		<finalCladeCutoff>75.0</finalCladeCutoff>
	</cladeCategory>

	<cladeCategory>
	
		<name>genotype</name>	
		<displayName>Genotype</displayName>	
		<whereClause>displayName like '3%' and clade_category = 'genotype'</whereClause>
		<distanceScalingExponent>-4.5</distanceScalingExponent>
		<distanceCutoff>0.02</distanceCutoff>
		<finalCladeCutoff>65.0</finalCladeCutoff>
		
	</cladeCategory>

</maxLikelihoodGenotyper>



